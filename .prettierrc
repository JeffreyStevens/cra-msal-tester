{
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "bracketSpacing": false,
    "arrowParens": "always",
    "printWidth": 132,
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "html"
            }
        }
    ]
}
